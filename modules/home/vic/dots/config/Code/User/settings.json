{
    "vspacecode.bindingOverrides": [
        {
          "keys": "f.b",
          "name": "FileBrowser Open",
          "type": "command",
          "command":"file-browser.open",
        },
        {
          "keys": "p.e",
          "name": "Yazi",
          "type": "command",
          "command":"vscodeyazi.openYazi",
        },
        {
          "keys": "p.F",
          "name": "nix fmt",
          "type": "command",
          "command": "runCommands",
            "args": {
              "commands": [
                "workbench.action.terminal.focus",
                {
                    "command": "workbench.action.terminal.sendSequence",
                    "args": {
                      "text": "nix fmt\n"
                    }
                }
              ]
            }
        },
        {
          "keys": "g.g",
          "name": "LazyGit",
          "type": "command",
          "command":"lazygit.openLazygit",
        },
        {
          "keys": "f.f",
          "name": "Fuzzy find file in project",
          "type": "command",
          "command":"find-it-faster.findFiles",
        },
        {
          "keys": "f.p",
          "name": "Fuzzy find file in project",
          "type": "command",
          "command":"find-it-faster.findFiles",
        },
        {
          "keys": "s.p",
          "name": "Fuzzy search in project",
          "type": "command",
          "command":"find-it-faster.findWithinFiles",
        },
        {
          "keys": "s.f",
          "name": "Fuzzy search same file type in project",
          "type": "command",
          "command":"find-it-faster.findWithinFilesWithType",
        }
    ],
    "window.customTitleBarVisibility": "windowed",
    "editor.minimap.enabled": false,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.copyOnSelection": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "git.confirmSync": false,
    "editor.inlineSuggest.edits.showCollapsed": true,
    "workbench.colorTheme": "Midnight Purple 2077",
    "editor.renderLineHighlight": "gutter",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "vim.smartRelativeLine": true,
    "editor.lineNumbers": "relative",
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "editor.fontSize": 18,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "find-it-faster.general.useTerminalInEditor": true,
    "find-it-faster.findFiles.previewWindowConfig": "right,50%,border-left,+{2}+3/3,~3",
    "find-it-faster.findWithinFiles.fuzzRipgrepQuery": true,
    "find-it-faster.findWithinFiles.previewWindowConfig": "right,50%,border-left,+{2}+3/3,~3",
    "find-it-faster.advanced.disableStartupChecks": true,
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "scm.diffDecorationsGutterVisibility": "hover",
    "zenMode.showTabs": "none",
    "terminal.integrated.sendKeybindingsToShell": true,
    "find-it-faster.general.killTerminalAfterUse": true,
    "find-it-faster.general.openFileInPreviewEditor": true,
    "direnv.restart.automatic": true,
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Github Light Theme",
    "workbench.preferredDarkColorTheme": "Midnight Purple 2077",
    "workbench.view.showQuietly": {
        "workbench.panel.output": false
    },
    "workbench.editor.centeredLayoutFixedWidth": true,
    "workbench.editor.limit.enabled": false,
    "workbench.editor.limit.value": 1,
    "errorLens.statusBarColorsEnabled": true,
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "top",
    "workbench.editor.showTabs": "none",
    "workbench.editor.limit.excludeDirty": true,
    "workbench.editor.limit.perEditorGroup": true,
    "cursor.cmdk.useThemedDiffBackground": true,
    "search.showLineNumbers": true,
    "editor.fontFamily": "'JetBrainsMono NFM Light', 'Droid Sans Mono', 'monospace', monospace",
    "git.enabled": false,
    "opensource-explorer.defaultClonePath": "~/hk",
    "opensource-explorer.defaultView": "recent",
    "debug.openExplorerOnEnd": true
}